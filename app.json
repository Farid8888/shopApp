{
  "expo": {
    "name": "shopAPP",
    "slug": "shopAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/2935183.png",
    "splash": {
      "image": "./assets/2935183.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farid8888.shopAPP"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.farid8888.shopAPP"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./local/assets/2935183.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
